{
  "name": "cozy-files",
  "version": "0.1.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Cozy files allows to store files in your cozy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/poupotte/cozy-files.git"
  },
  "main": "server.js",
  "dependencies": {
    "americano": "0.2.12",
    "americano-cozy": "0.1.10",
    "async": "0.2.9",
    "cozy-realtime-adapter": "0.12.0",
    "cozy-i18n-helper": "0.0.4",
    "jade": "0.35.0",
    "archiver": "~0.4.10"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0",
    "request-json": ">= 0",
    "coffee-script": "latest"
  },
  "scripts": {
    "test": "cake tests",
    "start": "coffee server.coffee"
